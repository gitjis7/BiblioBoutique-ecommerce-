                    </li>
                </ul>

            </nav>

            <!-- Page Content  -->
            <div id="content" class="p-4 p-md-5 pt-5">


                <section>

                    <div class=" ml-5 w-50 col-md-6 ">

                        <hr>

                        <form method="post" action="/useraddress-edit">
                            <div class="form-row">




                                <div class="form-group col-md-6">
                                    <label for="inputEmail4">Recipient name: <span
                                            style="color: crimson;">*</span></label>
                                    <input type="text" class="form-control" id="inputEmail4" name="name" required
                                        value="<%= userAddress.name%>">
                                </div>
                                <div class="form-group  col-md-6">
                                    <label for="inputAddress">Phone:<span style="color: crimson;">*</span></label>
                                    <input type="number" class="form-control" id="inputAddress" name="phone" required
                                        value="<%= userAddress.phone%>">
                                </div>
                            </div>
 
                            <div class="row">


                                <div class="form-group  col-md-12">
                                    <label for="inputAddress">Email address:<span
                                            style="color: crimson;">*</span></label>
                                    <input type="email" class="form-control" id="inputAddress" name="email" required
                                        value="<%= userAddress.email%>">
                                </div>

                            </div>
                            <div class="form-group">
                                <label for="inputAddress">Street Address:<span style="color: crimson;">*</span></label>
                                <input type="text" class="form-control" id="inputAddress" name="streetaddress" required
                                    value="<%= userAddress.streetaddress%>">
                            </div>
                            <div class="form-group">
                                <label for="inputAddress2">Landmark:<span style="color: crimson;">*</span></label>
                                <input type="text" class="form-control" id="inputAddress2" name="landmark" required
                                    value="<%= userAddress.landmark%>">
                            </div>

                            <div class="form-group">
                                <label for="inputCountry">Country:<span style="color: crimson;">*</span></label>
                                <select id="inputCountry" class="form-control" name="country" required >
                                  <option selected disabled>India</option>
                            
                                </select>
                              </div>

                           <div class="form-group">
               <label for="inputState">State:<span style="color: crimson;">*</span></label>
               <select id="inputState" class="form-control" name="state" required>
                 <option selected disabled>Choose...</option>
          <% States.forEach(state=>{ %>

        
                  <option ><%= state.name %></option>
    
                  <% }) %>
               </select>
             </div>

                            <div class="form-group">
                                <label for="inputCity">City:<span style="color: crimson;">*</span></label>
                                <input type="text" class="form-control" id="inputCity" name="city" required
                                    value="<%= userAddress.city%>">
                            </div>

                            <div class="form-group">
                                <label for="inputAddress">Address Line 1:</label>
                                <input type="text" class="form-control" id="inputAddress" placeholder="optional"
                                    name="addressline1">
                            </div>

                            <div class="form-group">
                                <label for="inputAddress">Address Line 2:</label>
                                <input type="text" class="form-control" id="inputAddress" placeholder="optional"
                                    name="addressline2">
                            </div>

                            <div class="form-group ">
                                <label for="inputZip">Zipcode:<span style="color: crimson;">*</span></label>
                                <input type="text" class="form-control" id="inputZip" name="zipcode" required
                                    value="<%= userAddress.zipcode%>">
                            </div>


                            <button type="submit" class="btn btn-primary mb-5 d-block mx-auto ">Save changes</button>
                        </form>




                    </div>



                </section>


            </div>
        </div>






    </main>









    <a class="scroll-to-top rounded" href="#page-top">
