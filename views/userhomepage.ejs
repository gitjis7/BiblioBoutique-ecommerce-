                            <h3>
                              <%= item.bookname %>
                            </h3>
                            <p>By <%= item.author %>
                            </p>
                            <div class="price">
                              <span class="text-danger">₹<%= item.
                                discountPrice %></span>
                            </div>
                            <div class="review">
                              <div class="rating">
                                <i class="fas fa-star"></i>
                                <i class="fas fa-star"></i>
                                <i class="fas fa-star"></i>
                                <i class="fas fa-star"></i>
                                <i class="fas fa-star-half-alt"></i>
                              </div>
                              <p>(120 Review)</p>
                            </div>
                            <a href="#" class="border-btn">View Details</a>
                          </div>
                        </div>
                        <% }else { %>




                          <% } %>
                            <% }) %>
