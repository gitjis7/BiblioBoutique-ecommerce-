                  <i class="fas fa-star"></i>
                  <i class="fas fa-star-half-alt"></i>
                </div> -->
            </div>
          </div>
        </div>
      </div>

      <div class="container">
        <div class="row justify-content-center">
          <% books.forEach((item) => { %>
            <% if (item.isListed) { %>
              <div class="card text-center mb-5 ml-2 custom-card" style="width: 13rem;">
                <a href="/books/bookInfo/<%= item.id %>">
                  <img class="card-img-top" src="<%= item.Image[0].slice('./public'.length) %>" alt="Card image cap">
                </a>
                <div class="card-body">
                  <p class="card-text custom-bookname"><%= item.bookname %></p>
                  <p class="card-text custom-author"><%= item.author %></p>
                  <p class="card-text custom-price text-danger">₹<%= item.
                    discountPrice %></p>
                </div>
              </div>
            <% } else { %>
              <!-- Add code for non-listed items if needed -->
            <% } %>
          <% }) %>
        </div>
      </div>












      <div class="services-area2 top-padding">
